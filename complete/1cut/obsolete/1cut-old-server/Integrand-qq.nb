         RowBox[{"{", 
          RowBox[{
           RowBox[{"TT", "[", 
            RowBox[{"x_", ",", "y_", ",", "d_", ",", "w_"}], "]"}], ",", 
           "z___"}], "}"}], ":>", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"TT", "[", 
            RowBox[{"x", ",", 
             SubscriptBox["a", "w"], ",", "d", ",", "w"}], "]"}], ",", "z"}], 
          "}"}]}], "}"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Caux", "=", 
      RowBox[{"Caux", "//.", 
       RowBox[{"{", 
        RowBox[{
