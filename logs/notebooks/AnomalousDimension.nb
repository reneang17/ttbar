  3.635226242083704*^9, 3.635226252747551*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"G", "[", "mu_", "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"a", "[", "mu", "]"}], 
     RowBox[{"G0", "[", "mu", "]"}]}], "+", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "[", "mu", "]"}], "^", "2"}], 
     RowBox[{"G1", "[", "mu", "]"}]}]}]}], ";", 
  RowBox[{
   RowBox[{"Z", "[", "mu_", "]"}], "=", 
   RowBox[{"1", "+", 
    RowBox[{
     RowBox[{"a", "[", "mu", "]"}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"g10", "/", 
        RowBox[{"(", 
         RowBox[{"2", 
          RowBox[{"ep", "^", "2"}]}], ")"}]}], "+", 
       RowBox[{
        RowBox[{"G0", "[", "mu", "]"}], "/", 
        RowBox[{"(", 
         RowBox[{"2", "ep"}], ")"}]}]}], ")"}]}], "+", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "[", "mu", "]"}], "^", "2"}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"g10", "^", "2"}], "/", 
        RowBox[{"(", 
         RowBox[{"8", 
          RowBox[{"ep", "^", "4"}]}], ")"}]}], "+", 
       RowBox[{
        RowBox[{"g10", "/", 
         RowBox[{"(", 
          RowBox[{"4", 
           RowBox[{"ep", "^", "3"}]}], ")"}]}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"G0", "[", "mu", "]"}], "-", 
          RowBox[{
           RowBox[{"3", "/", "2"}], "b0"}]}], ")"}]}], "+", 
       RowBox[{
        RowBox[{
         RowBox[{"G0", "[", "mu", "]"}], "/", 
         RowBox[{"(", 
          RowBox[{"8", 
           RowBox[{"ep", "^", "2"}]}], ")"}]}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"G0", "[", "mu", "]"}], "-", 
          RowBox[{"2", "b0"}]}], ")"}]}], "+", 
       RowBox[{"g11", "/", 
        RowBox[{"(", 
         RowBox[{"8", 
          RowBox[{"ep", "^", "2"}]}], ")"}]}], "+", 
       RowBox[{
        RowBox[{"G1", "[", "mu", "]"}], "/", 
        RowBox[{"(", 
         RowBox[{"4", "ep"}], ")"}]}]}], ")"}]}]}]}], ";"}]], "Input",
