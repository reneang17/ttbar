     RowBox[{
      RowBox[{
       RowBox[{"G0", "[", "mu", "]"}], "\[Rule]", 
       RowBox[{"g00", "+", 
        RowBox[{"g10", " ", "L"}], "+", "C0"}]}], ",", 
      RowBox[{
       RowBox[{"G1", "[", "mu", "]"}], "\[Rule]", 
