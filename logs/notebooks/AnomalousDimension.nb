           RowBox[{"Series", "[", 
            RowBox[{
             RowBox[{"Expand", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"mu", " ", 
                    RowBox[{"D", "[", 
                    RowBox[{
                    RowBox[{"F", "[", "mu", "]"}], ",", "mu"}], "]"}]}], "-", 
                   
                   RowBox[{
                    RowBox[{"G", "[", "mu", "]"}], 
                    RowBox[{"F", "[", "mu", "]"}]}]}], ")"}], "/", 
                 RowBox[{
                  RowBox[{"a", "[", "mu", "]"}], "^", 
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"n", "/", "2"}], "+", "1"}], ")"}]}]}], "/.", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"G0", "[", "mu", "]"}], "\[Rule]", 
                   RowBox[{"g00", "+", 
                    RowBox[{"g10", " ", 
                    RowBox[{"Log", "[", 
                    RowBox[{
                    RowBox[{"mu", "^", "2"}], "/", 
                    RowBox[{"mu0", "^", "2"}]}], "]"}]}], "+", "C0"}]}], ",", 
                  
                  RowBox[{
                   RowBox[{"G1", "[", "mu", "]"}], "\[Rule]", 
                   RowBox[{"g01", "+", 
                    RowBox[{"g11", " ", 
                    RowBox[{"Log", "[", 
                    RowBox[{
                    RowBox[{"mu", "^", "2"}], "/", 
                    RowBox[{"mu0", "^", "2"}]}], "]"}]}], "+", "C1"}]}]}], 
