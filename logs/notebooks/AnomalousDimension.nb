Cell[BoxData[
 RowBox[{
  RowBox[{"scale", "=", 
   RowBox[{
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{"Cases", "[", 
       RowBox[{
        RowBox[{"Collect", "[", 
         RowBox[{
